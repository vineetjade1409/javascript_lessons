var listA = [1 , 7 , 6 , 3 , 4, 9, 0 ,2, 5, 8];
var listB = [1 , 6, 3, 5 , 65 , 97 , 28];

var uniqueNumbers = [];
var x = 0;
var output = [];
for(i = 0; i < listA.length; i++){
    for(j = 0; j < listB.length; j++){
       if(listA[i] ==listB[j] ){
           x =listA[i]
            listA[i].push(uniqueNumbers)
       };
       for(f = 0; f < uniqueNumbers.length;f++){
            if(x != uniqueNumbers[f])
            output.push(x)
       };
    };
};
console.log(output)
